{
  "name": "koa2-ts-cli",
  "version": "0.0.2",
  "description": "koa cli",
  "author": "kezhongfa",
  "keywords": [
    "koa",
    "ts",
    "koa-cli"
  ],
  "main": "index.js",
  "bin": {
    "koa2ts": "./bin/www"
  },
  "scripts": {
    "start": "node ./bin/www"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo '执行代码检测:' && lint-staged",
      "commit-msg": "echo '执行git提交信息格式检测:' && commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js}": [
      "git add ."
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kezhongfa/koa2-ts-cli.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kezhongfa/koa2-ts-cli/issues"
  },
  "homepage": "https://github.com/kezhongfa/koa2-ts-cli#readme",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "colors": "^1.4.0",
    "commitlint-config-fa": "^1.0.5",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "minimist": "^1.2.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "commander": "^6.1.0",
    "download-git-repo": "^3.0.2",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "object-merge": "^2.5.1",
    "ora": "^5.1.0",
    "tmp": "^0.2.1"
  }
}
